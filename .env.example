# EmailJS Configuration
# Copy this file to .env and update with your actual EmailJS credentials

# EmailJS Service Configuration
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Email Settings
VITE_EMAILJS_TO_EMAIL=your_email@example.com
VITE_EMAILJS_FROM_NAME=Your App Name

# Rate Limiting Settings (optional - defaults provided)
VITE_EMAILJS_MAX_EMAILS_PER_HOUR=10
VITE_EMAILJS_COOLDOWN_MINUTES=5